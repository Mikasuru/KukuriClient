{
  "name": "kukuri-client",
  "version": "1.0.3",
  "description": "A selfbot that wont betray you. [Copyright Â© Mikasuru 2024]",
  "main": "Main.js",
  "author": {
    "name": "Mikasuru",
    "email": "kukuri.dev@gmail.com",
    "url": "https://www.youtube.com/@kukuri_xyz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mikasuru/KukuriClient/issues"
  },
  "github": "https://github.com/Mikasuru/KukuriClient",
  "scripts": {
    "cli": "bun run Main.js",
    "setup": "bun run ./Module/Setup.js",
    "install": "bun run ./Module/Install.js",
    "start": "bun run install && bun run cli",
    "build": "bun run ./Module/Build.js",
    "update": "bun run ./Module/Update.js"
  },
  "dependencies": {
    "@distube/ytdl-core": "4.15.8",
    "2captcha": "^3.0.5-2",
    "axios": "1.7.7",
    "discord.js-selfbot-v13": "3.4.6",
    "express": "4.21.1",
    "node-notifier": "10.0.1",
    "sharp": "0.33.5",
    "systeminformation": "5.23.25",
    "universal-speedtest": "3.0.0",
    "ytdl-core": "4.11.5"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^24.9.1",
    "electron-store": "^8.1.0",
    "nexe": "^4.0.0-rc.6",
    "pkg": "^5.8.1"
  },
  "build": {
    "appId": "com.kukuri.client",
    "productName": "Kukuri Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "Application/**/*",
      "Server/**/*",
      "Module/**/*",
      "Config/**/*",
      "Commands/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "uninstallDisplayName": "Kukuri Client",
      "createStartMenuShortcut": true
    }
  }
}
